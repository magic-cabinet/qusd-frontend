{
  "buildCommand": "bun run build",
  "installCommand": "bun install",
  "trailingSlash": false,
  "rewrites": [
    { "source": "/whitepaper", "destination": "/index.html" },
    { "source": "/docs", "destination": "/index.html" },
    { "source": "/docs/:path*", "destination": "/index.html" }
  ]
}
